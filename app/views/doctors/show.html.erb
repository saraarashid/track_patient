<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @doctor.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @doctor.last_name %>
</p>

<p>
  <strong>License no:</strong>
  <%= @doctor.license_no %>
</p>

<p>
  <strong>Email:</strong>
  <%= @doctor.email %>
</p>

<p>
  <strong>Phone number:</strong>
  <%= @doctor.phone_number %>
</p>
<p>
  <span>Patients</span>
<%  @doctor.patients.each do |f| %>
<%= f.first_name %>
<%= f.last_name %>
<%= f.email %>
<%= f.phone_number %>
<%end%>
</p>

<%= link_to 'Edit', edit_doctor_path(@doctor) %> |
<%= link_to 'Back', doctors_path %>
