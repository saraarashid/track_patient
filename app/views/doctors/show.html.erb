<p id="notice"><%= notice %></p><br>
<div class="bg-success" >
<p class="bg-success">
  <strong>First name:</strong>
  <%= @doctor.first_name %>
</p>

<p class="bg-info">
  <strong>Last name:</strong>
  <%= @doctor.last_name %>
</p>

<p>
  <strong>License no:</strong>
  <%= @doctor.license_no %>
</p>

<p  class="bg-danger">
  <strong>Email:</strong>
  <%= @doctor.email %>
</p>

<p class="bg-info">
  <strong>Phone number:</strong>
  <%= @doctor.phone_number %>
</p>
<p>
  <span><strong>Patients:</strong></span>
<%  @doctor.patients.each do |f| %>
<%= f.first_name %>
<%= f.last_name %>
<%= f.email %>
<%= f.phone_number %>
<%end%>
</p>
</div>
<%= link_to 'Edit', edit_doctor_path(@doctor),  class:"btn btn-default" %> 
<%= link_to 'Back', doctors_path,  class:"btn btn-default" %>
